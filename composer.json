{
  "name": "fungku/netsuite-php-toolkit",
  "description": "Netsuite PHP Toolkit Package",
  "keywords": [ "netsuite", "php", "composer", "package" ],
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Ryan Winchester (fungku)",
      "email": "fungku@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "ext-soap": "*",
    "ext-openssl": "*",
    "ext-SimpleXML": "*"
  },
  "autoload": {
    "psr-0": {
      "NetSuiteClient": "src/",
      "NetSuiteService": "src/"
    },
    "psr-4": {
      "": "src/Classes/"
    },
    "files": [
      "src/includes/functions.php"
    ]
  },
  "minimum-stability": "stable"
}
